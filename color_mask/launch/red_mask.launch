<launch>

  <group ns="jetbot">

      <!-- remap the color_mask_node -->
      <node pkg="color_mask" name="red_mask_node" type="color_mask.py">

          <rosparam>
              HSV/LOWER: [0, 135, 67]
              HSV/UPPER: [179, 234, 112]
          </rosparam>

          <remap from="in_topic" to="camera/processed/cropped"/>
          <remap from="out_topic" to="red_mask"/>
      </node>

  </group>

</launch>
